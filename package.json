{
  "name": "@github-docs/render-content",
  "version": "0.0.0-development",
  "description": "Markdown and Liquid rendering pipeline",
  "repository": "https://github.com/docs/render-content",
  "license": "MIT",
  "scripts": {
    "lint": "standard --fix && prettier-standard --format **/*.js",
    "test": "npm run lint && tap test/*.js && npm run test:deps",
    "test:deps": "dependency-check . --missing --no-dev && dependency-check . --unused --no-dev"
  },
  "dependencies": {
    "@primer/octicons": "^9.1.1",
    "cheerio": "^1.0.0-rc.3",
    "html-entities": "^1.2.1",
    "hubdown": "^2.1.0",
    "liquid": "^4.0.0",
    "semver": "^5.7.1",
    "strip-html-comments": "^1.0.0"
  },
  "devDependencies": {
    "dependency-check": "^4.1.0",
    "prettier-standard": "^14.0.5",
    "semantic-release": "^17.0.4",
    "standard": "^14.3.1",
    "tap": "^14.6.4"
  }
}
